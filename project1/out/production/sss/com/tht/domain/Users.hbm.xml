<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.tht.domain">

    <class name="Users">
        <id name="id" column="id" type="java.lang.String">
            <generator class="native"/>
    </id>
        <property name="name" column="name" type="java.lang.String"/>
        <property name="sex" column="sex" type="org.hibernate.type.EnumType"/>
        <property name="age" column="age" type="java.lang.Integer"/>
        <property name="passwd" column="passwd" type="java.lang.String"/>
        <property name="height" column="height" type="java.lang.Double"/>
    </class>
</hibernate-mapping>